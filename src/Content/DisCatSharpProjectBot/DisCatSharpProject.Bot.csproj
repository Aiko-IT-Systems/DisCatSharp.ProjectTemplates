<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<TargetFramework>net6.0</TargetFramework>
		<ImplicitUsings>enable</ImplicitUsings>
		<Nullable>enable</Nullable>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="DisCatSharp" Version="10.2.0" />
		<PackageReference Include="DisCatSharp.Configuration" Version="10.2.0" />
		<PackageReference Include="DisCatSharp.Hosting" Version="10.2.0" />
		<PackageReference Include="DisCatSharp.Hosting.DependencyInjection" Version="10.2.0" />
		<PackageReference Include="Microsoft.Extensions.Configuration.Json" Version="7.0.0" />
	</ItemGroup>

	<!--#if (UseApplicationCommands) -->
	<ItemGroup>
		<PackageReference Include="DisCatSharp.ApplicationCommands" Version="10.2.0" />
	</ItemGroup>
	
	<!--#endif -->
	<!--#if (UseCommon) -->
	<ItemGroup>
		<PackageReference Include="DisCatSharp.Common" Version="10.2.0" />
	</ItemGroup>

	<!--#endif -->
	<!--#if (UseInteractivity) -->
	<ItemGroup>
		<PackageReference Include="DisCatSharp.Interactivity" Version="10.2.0" />
	</ItemGroup>
	
	<!--#endif -->
	<!--#if (UseLavalink) -->
	<ItemGroup>
		<PackageReference Include="DisCatSharp.Lavalink" Version="10.2.0" />
	</ItemGroup>
	
	<!--#endif -->
	<!--#if (UseVoice) -->
	<ItemGroup>
		<PackageReference Include="DisCatSharp.VoiceNext" Version="10.2.0" />
	</ItemGroup>
	
	<!--#endif -->
	<!--#if (UseVoiceNatives) -->
	<ItemGroup>
		<PackageReference Include="DisCatSharp.VoiceNext.Natives" Version="10.2.0" />
	</ItemGroup>
	<ItemGroup>
	  <None Update="translations\my_simple_command.json">
	    <CopyToOutputDirectory>Always</CopyToOutputDirectory>
	  </None>
	</ItemGroup>
	
	<!--#endif -->
</Project>
